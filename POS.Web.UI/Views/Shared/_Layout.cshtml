            $(".form-control").addClass('txtboxRadius');
          //  $("body").removeCss("font-family: 'Open Sans', sans-serif;");
            ShowAlert();
        });

        function ShowAlert() {
            var msg = '@TempData["Message"]';
            if (msg != null) {
                var type = '@TempData["AlertType"]';
                if (type == "Success") {
                    SucccesAlert(msg);
                }
                else if (type == "Error") {
                    ErrorAlert(msg);
                }
                else if (type == "Warning") {
                    WarningAlert(msg);
                }
                else if (type == "Info") {
                    InfoAlert(msg);
                }
            }
        }
        function SucccesAlert(msg) {
            toastr.success(msg)
        }
        function ErrorAlert(msg) {
            toastr.error(msg)
        }
        function InfoAlert(msg) {
            toastr.info(msg)
        }
        function WarningAlert(msg) {
            toastr.warning(msg)
        }
    </script>
