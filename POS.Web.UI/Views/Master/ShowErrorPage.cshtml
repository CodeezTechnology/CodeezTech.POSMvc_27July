@model CodeezTech.POS.CommonProject.Error

@{
    Layout = null;
}

<!DOCTYPE html>

<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="images/favicon.png">
    <title>Error</title>
    @Styles.Render("~/Content/css");
    @Scripts.Render("~/Scripts/js");

</head>

<body class="fix-header fix-sidebar">
    <!-- Preloader - style you can find in spinners.css -->
    <div class="preloader">
        <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
        </svg>
    </div>
    <!-- Main wrapper  -->
    <div class="error-page" id="wrapper">
        <div class="error-box">
            <div class="error-body text-center">
                <h1>ERROR</h1>
                <h3 class="text-uppercase">@Model.Breadcrum </h3>
                <p class="text-muted m-t-30 m-b-30">@Model.ErrorMsg</p>
                <a class="btn btn-info btn-rounded waves-effect waves-light m-b-40" href="index.html">Back to home</a>
                <button class="btn btn-primary btn-rounded waves-effect waves-light m-b-40"  type="button" onclick="history.back();">Previous Page</button>
            </div>
            <footer class="footer text-center">&copy; 2018 CodeezTech.</footer>
        </div>
    </div>
    <!-- End Wrapper -->
  
   
  
</body>

</html>